{
  "name": "pokedex-api",
  "version": "1.0.0",
  "description": "Pokedex api",
  "main": "src/external/server.ts",
  "repository": "https://github.com/Lissone/pokedex.git",
  "author": "Leonardo Dias Lissone Santomero",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/external/server.js",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "heroku-postbuild": "npm run build",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register ."
  },
  "heroku-run-build-script": true,
  "dependencies": {
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "firebase": "^8.6.8",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/node": "^15.14.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "ts-node-dev": "^1.1.7",
    "tscpaths": "^0.0.9",
    "typescript": "^4.3.5"
  }
}